RESET$
READ;FILE="C:\Data\data_FADNPanelAnalysis\OutData\multisward\exported_data.csv"$



SORT		;LHS=PID;RHS=*									$
CREATE	 	;IND=NDX(PID,0)									$
REGRESS	;LHS=ONE;RHS=ONE;STR=PID;PANEL							$
CREATE		;GRPSIZE=_GROUPTI									$
