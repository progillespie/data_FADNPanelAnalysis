		;HALTON;PTS=30;RPM;FCN=ONE(n);HFU=FDRATIO; HFV=FDRATIO; EFF=UTRE    	$


? TFE model Greene(2005)
FRONTIER	;LHS=LNTOTALI;RHS=COBBDGLS;PDS=GRPSIZE						$
FRONTIER	;LHS=LNTOTALI;RHS=COBBDGLS;PDS=GRPSIZE
		;HALTON;PTS=30;FEM;FCN=ONE(n);HFU=FDRATIO; HFV=FDRATIO; EFF=UTRE		$

KERNEL		; RHS=UTRE										$ 
